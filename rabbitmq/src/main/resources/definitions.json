{
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "users": [
    {
      "name": "${RABBITMQ_USER_VALUE}",
      "password": "${RABBITMQ_PASSWORD}",
      "tags": "administrator"
    }
  ],
  "permissions": [
    {
      "user": "${RABBITMQ_USER_VALUE}",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "policies": [
    {
      "vhost": "/",
      "name": "VRO-GLOBAL-QUEUES",
      "pattern": ".*",
      "apply-to": "queues",
      "definition": {
        "dead-letter-exchange": "vro.dlx",
        "max-length": 1000
      },
      "priority": 0
    },
    {
      "vhost": "/",
      "name": "VRO-GLOBAL-MESSAGE-TTL",
      "pattern": ".*",
      "apply-to": "queues",
      "definition": {
        "message-ttl": 1500000
      },
      "priority": 0
    }
  ]
}
