  "components" : {
    "schemas" : {
      "ResourceRequest" : {
        "required" : [ "diagnosticCode", "resourceId" ],
        "type" : "object",
        "properties" : {
          "resourceId" : {
            "type" : "string",
            "description" : "resource ID",
            "example" : "1234"
          },
          "diagnosticCode" : {
            "type" : "string",
            "description" : "Diagnostic code",
            "example" : "A"
          }
        },
        "description" : "payload for resource"
      },
      "ResourceResponse" : {
        "required" : [ "resourceId" ],
        "type" : "object",
        "properties" : {
          "resourceId" : {
            "type" : "string"
          },
          "diagnosticCode" : {
            "type" : "string"
          },
          "status" : {
            "type" : "string"
          },
          "statusCode" : {
            "type" : "integer",
            "format" : "int32"
          },
          "statusMessage" : {
            "type" : "string"
          }
        }
      },
      "HealthDataAssessmentRequest" : {
        "required" : [ "claimSubmissionId", "diagnosticCode", "veteranIcn" ],
        "type" : "object",
        "properties" : {
          "veteranIcn" : {
            "type" : "string",
            "description" : "Veteran medical internal control number (EHR id)",
            "example" : "9000682"
          },
          "diagnosticCode" : {
            "type" : "string",
            "description" : "Diagnostic code for the claim contention",
            "example" : "7101"
          },
          "claimSubmissionId" : {
            "type" : "string",
            "description" : "Claim submission id",
            "example" : "1234"
          },
          "disabilityActionType" : {
            "type" : "string",
            "description" : "MAS",
            "example" : "NEW"
          },
          "claimSubmissionDateTime" : {
            "type" : "string",
            "description" : "Date of the Claim"
          }
        },
        "description" : "Claim for which health data assessment requested"
      },
      "AbdBloodPressure" : {
        "type" : "object",
        "properties" : {
          "date" : {
            "type" : "string",
            "description" : "The date blood pressure is taken",
            "example" : "1999-12-31"
          },
          "diastolic" : {
            "$ref" : "#/components/schemas/AbdBpMeasurement"
          },
          "systolic" : {
            "$ref" : "#/components/schemas/AbdBpMeasurement"
          },
          "practitioner" : {
            "type" : "string",
            "description" : "Name of the physician who took the measurement",
            "example" : "DR. THOMAS REYNOLDS PHD"
          },
          "organization" : {
            "type" : "string",
            "description" : "Location where the measurement taken",
            "example" : "WASHINGTON VA MEDICAL CENTER"
          },
          "document" : {
            "type" : "string",
            "description" : "Document Type Description",
            "example" : "Medical Treatment Record - Government Facility"
          },
          "receiptDate" : {
            "type" : "string",
            "description" : "VBMS Receipt Date",
            "example" : "1999-12-31"
          },
          "page" : {
            "type" : "string",
            "description" : "Document Page Number",
            "example" : "55"
          },
          "documentId" : {
            "type" : "string",
            "description" : "Document Identifier",
            "example" : "{BFA4943C-4F56-4AC5-B48F-5FDE469B1226}"
          },
          "dateFormatted" : {
            "type" : "string",
            "description" : "Formatted date",
            "example" : "12/31/1999"
          },
          "dataSource" : {
            "type" : "string",
            "description" : "Source of this data",
            "example" : "LH"
          },
          "partialDate" : {
            "type" : "string",
            "description" : "Partial date from OCR",
            "example" : "**/**/1988"
          }
        },
        "description" : "List of relevant blood pressures"
      },
      "AbdBpMeasurement" : {
        "type" : "object",
        "properties" : {
          "code" : {
            "type" : "string",
            "description" : "Code for measurement type",
            "example" : "8480-6"
          },
          "display" : {
            "type" : "string",
            "description" : "Display name for measurement type",
            "example" : "Systolic blood pressure"
          },
          "unit" : {
            "type" : "string",
            "description" : "Unit for the measurement",
            "example" : "mm[Hg]"
          },
          "value" : {
            "type" : "number",
            "description" : "Measurement value",
            "example" : 110
          }
        },
        "description" : "Systolic measurement"
      },
      "AbdCondition" : {
        "type" : "object",
        "properties" : {
          "text" : {
            "type" : "string"
          },
          "code" : {
            "type" : "string"
          },
          "abatementDate" : {
            "type" : "string"
          },
          "status" : {
            "type" : "string"
          },
          "onsetDate" : {
            "type" : "string"
          },
          "recordedDate" : {
            "type" : "string"
          },
          "relevant" : {
            "type" : "boolean"
          },
          "dateFormatted" : {
            "type" : "string",
            "description" : "Formatted date",
            "example" : "12/31/1999"
          },
          "dataSource" : {
            "type" : "string",
            "description" : "Source of this data",
            "example" : "LH"
          },
          "partialDate" : {
            "type" : "string",
            "description" : "Partial date from OCR",
            "example" : "**/**/1988"
          },
          "organization" : {
            "type" : "string",
            "description" : "Location where the measurement taken",
            "example" : "WASHINGTON VA MEDICAL CENTER"
          },
          "document" : {
            "type" : "string",
            "description" : "Document Identifier",
            "example" : "{BFA4943C-4F56-4AC5-B48F-5FDE469B1226}"
          },
          "receiptDate" : {
            "type" : "string",
            "description" : "VBMS Receipt Date",
            "example" : "1999-12-31"
          },
          "page" : {
            "type" : "string",
            "description" : "Document Page Number",
            "example" : "55"
          }
        },
        "description" : "List of relevant conditions"
      },
      "AbdEvidence" : {
        "type" : "object",
        "properties" : {
          "medications" : {
            "type" : "array",
            "description" : "List of relevant medications",
            "items" : {
              "$ref" : "#/components/schemas/AbdMedication"
